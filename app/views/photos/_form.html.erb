<%= simple_form_for(@photo, html: { multipart: true, class: "form-horizontal" }) do |f| %>
      <%= f.input :img, label: "图片" %>
      <%= f.file_field :img %>
    <div class="line line-dashed b-b line-lg pull-in"></div>
    <div class="col-sm-10 col-sm-offset-2">
      <%= f.button :submit, "提 交", class: "btn btn-primary" %>
      <%= link_to "返 回", photos_path, class: "btn btn-default" %>
  </div>
<% end %>
